{
  "_version": "1.81.0",
  "sap.app": {
    "id": "salesapp",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.20.1",
      "toolsId": "fd98b195-60c8-4508-89ed-dc5d2725d2fb"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/sales-order/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.144.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "salesapp.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "SalesNumberList",
          "target": "SalesNumberList"
        },
        {
          "pattern": "SalesNumber({key}):?query:",
          "name": "SalesNumberObjectPage",
          "target": "SalesNumberObjectPage"
        },
        {
          "pattern": "SalesNumber({key})/salesitems({key2}):?query:",
          "name": "salesitemsObjectPage",
          "target": "salesitemsObjectPage"
        }
      ],
      "targets": {
        "SalesNumberList": {
          "type": "Component",
          "id": "SalesNumberList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/SalesNumber",
              "variantManagement": "Page",
              "navigation": {
                "SalesNumber": {
                  "detail": {
                    "route": "SalesNumberObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  },
                  "actions": {
                    "Number": {
                      "press": "salesapp.ext.controller.Number.Number",
                      "visible": true,
                      "enabled": true,
                      "requiresSelection": false,
                      "text": "Number"
                    }
                  }
                }
              }
            }
          }
        },
        "SalesNumberObjectPage": {
          "type": "Component",
          "id": "SalesNumberObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/SalesNumber",
              "navigation": {
                "salesitems": {
                  "detail": {
                    "route": "salesitemsObjectPage"
                  }
                }
              }
            }
          }
        },
        "salesitemsObjectPage": {
          "type": "Component",
          "id": "salesitemsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/SalesNumber/salesitems"
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  },
  "sap.fe": {
    "app": {
      "enableLazyLoading": true
    }
  }
}
